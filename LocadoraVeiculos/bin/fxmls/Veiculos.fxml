<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane stylesheets="@../principal/application.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ControllerVeiculos">
   <children>
      <Pane prefHeight="620.0" prefWidth="1000.0" styleClass="fundoMasculo">
         <children>
            <TableView fx:id="tcVeiculos" layoutX="14.0" layoutY="81.0" prefHeight="489.0" prefWidth="968.0">
               <columns>
                  <TableColumn fx:id="tcIdVeiculo" prefWidth="32.0" text="#" />
                  <TableColumn fx:id="tcPlaca" prefWidth="102.0" text="Placa" />
                  <TableColumn fx:id="tcAnoFabricacao" prefWidth="151.0" text="Ano De Fabricação" />
                  <TableColumn fx:id="tcCor" prefWidth="94.0" text="Cor" />
                  <TableColumn fx:id="tcModelo" prefWidth="101.0" text="Modelo" />
                  <TableColumn fx:id="tcFabricante" prefWidth="162.0" text="Fabricante" />
                  <TableColumn fx:id="tcEstado" prefWidth="75.0" text="Estado" />
                  <TableColumn fx:id="tcProprietario" prefWidth="145.0" text="Proprietário" />
                  <TableColumn fx:id="tcStatus" prefWidth="105.0" text="Status" />
               </columns>
            </TableView>
            <Button fx:id="btCadastrarVeiculos" layoutX="875.0" layoutY="42.0" mnemonicParsing="false" onAction="#cadastrarVeiculos" styleClass="btn-success" text="Novo Veiculo" />
            <Button fx:id="btExcluirVeiculo" layoutX="636.0" layoutY="42.0" mnemonicParsing="false" onAction="#excluirVeiculo" styleClass="btn-danger" text="Excluir Veiculo" />
            <Button fx:id="btEditVeiculo" disable="true" layoutX="758.0" layoutY="42.0" mnemonicParsing="false" onAction="#editarVeiculo" styleClass="btn-primary" text="Editar Veiculo" />
         </children></Pane>
   </children>
</AnchorPane>
